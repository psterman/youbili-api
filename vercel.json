{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/video-info",
            "dest": "/api/video-info.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}